{
  "name": "proconnect-espace-partenaire",
  "description": "Espace partenaire de ProConnect",
  "version": "1.0.0",
  "private": true,
  "author": "DINUM",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=22"
  },
  "workspaces": [
    "packages/email"
  ],
  "scripts": {
    "build:workspaces": "npm run build --if-present --workspaces",
    "predev": "npm run prisma:generate && npm run build:workspaces && npm run only-include-used-icons",
    "prebuild": "npm run prisma:generate && node -r @swc-node/register scripts/prebuild.ts && npm run only-include-used-icons",
    "only-include-used-icons": "node node_modules/@codegouvfr/react-dsfr/bin/only-include-used-icons.js",
    "dev": "next dev",
    "build": "npm run build --if-present --workspaces && next build",
    "start": "next start",
    "lint": "prettier --write '**/*.{ts,tsx}' && next lint",
    "lint-staged": "lint-staged",
    "e2e": "NEXT_PUBLIC_BASE_PATH='' NODE_ENV=production npm run build && npm run playwright test",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:lint": "prettier '**/*.{ts,tsx}' --list-different",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "db_espace:reset": "dotenv -e .env.local -- prisma db push --schema=./prisma/db_espace.prisma",
    "db_espace:browse": "dotenv -e .env.local -- prisma studio --schema=./prisma/db_espace.prisma",
    "db_proconnect:browse": "dotenv -e .env.local -- prisma studio --schema=./prisma/db_proconnect.prisma",
    "db_proconnect:reset": "dotenv -e .env.local -- prisma db push --schema=./prisma/db_proconnect.prisma",
    "prisma:generate": "prisma generate --schema=./prisma/db_espace.prisma && prisma generate --schema=./prisma/db_proconnect.prisma",
    "db:reset": "bash scripts/generate-mongodb-keyfile.sh && docker-compose down -v && docker-compose up -d && sleep 5 && docker-compose exec mongodb mongosh -u fc -p pass --eval 'rs.initiate({_id:\"rs0\",members:[{_id:0,host:\"localhost:27017\"}]})' && sleep 2 && npm run db_espace:reset && npm run db_proconnect:reset"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "1.17.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@gouvfr-lasuite/proconnect.debounce": "0.4.1",
    "@gouvfr-lasuite/proconnect.email": "workspace:*",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/mdx": "^14.2.23",
    "@prisma/client": "^6.2.1",
    "@sentry/nextjs": "^7.120.2",
    "@shikijs/transformers": "^2.1.0",
    "@socialgouv/matomo-next": "^1.9.2",
    "bson": "^6.10.2",
    "html-to-text": "^9.0.5",
    "is-ci": "^3.0.1",
    "lodash": "^4.17.21",
    "next": "14.2.23",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.9.16",
    "prisma": "^6.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.33.5",
    "shiki": "^2.1.0",
    "tss-react": "^4.9.14"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@playwright/test": "^1.49.1",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.10.12",
    "@testing-library/react": "^14.3.1",
    "@types/lodash": "^4.17.15",
    "@types/mdx": "^2.0.13",
    "@types/node": "22.10.10",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv-cli": "^8.0.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.23",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jsdom": "24.1.3",
    "lint-staged": "15.3.0",
    "start-server-and-test": "2.0.9",
    "typescript": "5.4.3",
    "vitest": "1.6.0"
  },
  "optionalDependencies": {
    "@swc/core-linux-x64-gnu": "^1.10.12"
  }
}
